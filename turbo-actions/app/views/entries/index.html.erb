<p style="color: green"><%= notice %></p>

<h1>Entries</h1>

<%= turbo_frame_tag("entries") do %>
  <% @entries.each do |entry| %>
    <%= render entry %>
    <p>
      <%= link_to "Show this entry", entry %>
    </p>
  <% end %>
<% end %>

<%= render 'more_button' %>
<%= link_to "New entry", new_entry_path %>
